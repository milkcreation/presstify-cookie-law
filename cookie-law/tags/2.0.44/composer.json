{
  "name": "presstify-plugins/cookie-law",
  "description": "Cookie Law PresstiFy plugin - Privacy policy management.",
  "homepage": "https://svn.tigreblanc.fr/presstify-plugins/cookie-law/",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "cookie",
    "rgpd",
    "gprd"
  ],
  "type": "presstify-plugin",
  "time": "2020-11-22",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": ">=1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "tiFy\\Plugins\\CookieLaw\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tiFy\\Plugins\\ThemeSuite\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,  
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
